{
  "name": "server",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run build\" \"npm run server\"",
    "server": "nodemon ./dist/app.js",
    "serve": "nodemon --config nodemon-config.json --inspect dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc --w",
    "build": "rimraf dist && npm run build-ts",
    "migrate": "sequelize db:migrate",
    "rollback": "sequelize db:migrate:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts": "tsc -w",
    "lint": "tslint --fix --project tsconfig.json"
  },
  "author": "",
  "license": "ISC",
