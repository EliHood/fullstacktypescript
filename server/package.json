    "@types/bluebird": "^3.5.28",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/morgan": "^1.7.37",
    "@types/sequelize": "^4.28.5",
    "@types/validator": "^10.11.3",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
